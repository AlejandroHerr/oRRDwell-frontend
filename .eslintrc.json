{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
      "react/jsx-filename-extension": [0, { "extensions": [".js"] }],
      "react/prefer-stateless-function": [1, { "ignorePureComponents": true }],
      "react/jsx-sort-props": [1, { "callbacksLast": true }],
      "react/sort-prop-types": [1, { "callbacksLast": true }]
    }
}